# AI QA Orchestration Framework - Environment Variables
# Copy this file to .env and add your actual API keys

# Required: Anthropic API key (get it from: https://console.anthropic.com/)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: OpenAI API key (if you want to use OpenAI models)
# OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration (modify if needed)
REAL_APP_PORT=8501
DEMO_APP_PORT=8502

# Optional: Debug mode
DEBUG=false
ENVIRONMENT=development